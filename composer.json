{
    "name": "usf-it/armapi",
    "description": "Native service methods supporting the ARM project.",
    "type": "library",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "James Jones",
            "email": "james@usf.edu",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/USF-IT/usf-idm-database/"
        },
        {
            "type": "vcs",
            "url": "https://github.com/USF-IT/usf-idm-common/"
        },
        {
            "type": "vcs",
            "url": "https://github.com/USF-IT/VisorClientAPI.git"
        }
    ],
    "require": {
        "usf-it/usf-idm-database": "dev-master",
        "usf-it/usf-idm-common": "dev-master",
        "NanneHuiges/JSend": "2.1.0",
        "usf-it/usf-visor-client-api": "~1.0",
        "phpmailer/phpmailer": "~5.2",
        "smarty/smarty": "~3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "@stable",
        "squizlabs/php_codesniffer": "2.1.*",
        "zumba/mongounit": "dev-master",
        "phpunit/dbunit": ">=1.3.2"
    },
    "autoload": {
        "psr-4" : {
            "USF\\IdM\\" : [ "src/", "tests/" ]
        }
    }
